                    <child>
                      <widget class="Gtk.Label" id="PresetLabel">
                        <property name="MemberName" />
                        <property name="LabelProp" translatable="yes">Page</property>
                      </widget>
                      <packing>
                        <property name="type">tab</property>
                      </packing>
