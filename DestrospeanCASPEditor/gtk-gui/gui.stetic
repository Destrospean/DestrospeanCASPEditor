    <property name="MemberName" />
    <property name="WidthRequest">1080</property>
    <property name="HeightRequest">540</property>
    <property name="Title" translatable="yes">Destrospean's CASP Editor</property>
    <property name="WindowPosition">CenterOnParent</property>
    <property name="DefaultWidth">1080</property>
    <property name="DefaultHeight">540</property>
    <signal name="DeleteEvent" handler="OnDeleteEvent" />
    <child>
      <widget class="Gtk.HPaned" id="MainPane">
        <property name="MemberName" />
        <property name="CanFocus">True</property>
        <property name="Position">10</property>
        <child>
          <placeholder />
        </child>
        <child>
          <widget class="Gtk.VBox" id="MainVBox">
            <property name="MemberName" />
            <property name="Spacing">6</property>
            <child>
              <widget class="Gtk.MenuBar" id="MainMenuBar">
                <property name="MemberName" />
                <node name="MainMenuBar" type="Menubar">
                  <node type="Menu" action="FileAction">
                    <node type="Menuitem" action="NewAction" />
                    <node type="Menuitem" action="OpenAction" />
                    <node type="Menuitem" action="SaveAction" />
                    <node type="Menuitem" action="SaveAsAction" />
                    <node type="Menuitem" action="CloseAction" />
                    <node type="Menuitem" action="QuitAction" />
                  </node>
                  <node type="Menu" action="SettingsAction">
                    <node type="Menuitem" action="GameFoldersAction" />
                  </node>
                </node>
              </widget>
              <packing>
                <property name="Position">0</property>
                <property name="AutoSize">True</property>
                <property name="Expand">False</property>
                <property name="Fill">False</property>
              </packing>
            </child>
            <child>
